import java.util.Scanner;

public class MainClass {
	
	public static void main(String[] args)
	{
		MyGetSetClass obj=new MyGetSetClass();
		obj.setNum(36);
		System.out.println("My Number is : " + obj.getNum());
		
		InterFaceExtract objInter=new InterFaceExtract();
		objInter.NameDetails();
		Scanner sc = new Scanner(System.in);
		String str=sc.nextLine();
		sc.close();		
		System.out.println("Enter the Address : " +objInter.Address(str));
		objInter.DoorNum();
		
		
			
	}

}